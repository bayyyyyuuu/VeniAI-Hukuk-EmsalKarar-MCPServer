# Core
NODE_ENV=development
DATABASE_TYPE=postgresql
DATABASE_URL=postgresql://user:pass@host:5432/dbname?sslmode=require
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Browserless
BROWSERLESS_URL=wss://your-browserless-host
BROWSERLESS_TOKEN=your-token
BROWSERLESS_TIMEOUT=30000
BROWSERLESS_RETRIES=3

# Scraping
SCRAPING_MAX_RESULTS=10
SCRAPING_PARALLEL_REQUESTS=3
SCRAPING_ADAPTIVE_TIMEOUT=true
SCRAPING_RETRY_STRATEGY=exponential
SCRAPING_BASE_TIMEOUT=10000

# Cache
CACHE_L1_TTL_MS=300000
CACHE_L2_TTL_DAYS=30
CACHE_POPULAR_THRESHOLD=5
CACHE_CLEANUP_INTERVAL_MS=3600000
